{
  "name": "vesium-lib",
  "type": "module",
  "version": "1.0.1-beta.24",
  "private": true,
  "description": "Vue component and composition-api library for Cesium.",
  "license": "MIT",
  "homepage": "https://github.com/GeoVueJS/vesium",
  "repository": {
    "url": "https://github.com/GeoVueJS/vesium"
  },
  "bugs": {
    "url": "https://github.com/GeoVueJS/vesium/issues"
  },
  "keywords": [
    "vue",
    "cesium",
    "vueuse",
    "hooks",
    "composition-api"
  ],
  "scripts": {
    "docs:dev": "vitepress dev packages --host",
    "docs:build": "npm run build:types && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "build:types": "vue-tsc -p ./tsconfig.build.json --emitDeclarationOnly --declarationMap false --strict false",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "taze": "taze -w -r major minor patch",
    "release": "bumpp -r",
    "==========[CI/CD]===========": "",
    "prepublish": "tsx ./internals/build",
    "publish:ci": "pnpm publish -r --no-git-checks --access public"
  },
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "@unocss/reset": "^65.5.0",
    "@vesium/core": "workspace:*",
    "@vesium/plot": "workspace:*",
    "@vesium/shared": "workspace:*",
    "@vesium/special": "workspace:*",
    "@vueuse/core": "^12.7.0",
    "cesium": "^1.126.0",
    "pinia": "^3.0.1",
    "vesium": "workspace:*",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/tabler": "^1.2.16",
    "@iconify/utils": "^2.3.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^22.13.4",
    "@unocss/eslint-plugin": "^65.5.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^10.0.3",
    "cypress": "^14.0.3",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.0.0",
    "local-pkg": "^1.0.0",
    "magic-string": "^0.30.17",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "npm-run-all2": "^7.0.2",
    "pnpm": "^10.4.1",
    "prettier": "^3.5.1",
    "rollup": "^4.34.8",
    "rollup-plugin-esbuild": "^6.2.0",
    "sass": "1.78.0",
    "start-server-and-test": "^2.0.10",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "svgo": "^3.3.2",
    "taze": "^18.5.0",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.7",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "5.6.3",
    "unocss": "^65.5.0",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-cesium": "^2.1.0",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.1.0",
    "vite": "^6.1.0",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vitepress-sidebar": "^1.31.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.2"
  }
}
